{
  "name": "sst-analytics",
  "version": "0.0.3",
  "description": "AWS Lambda function retrieving analytics from Mailchimp and Google Analytics and sent to employees",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore spec.js,test.js",
    "test": "NODE_ENV=development tape 'src/**/*.spec.js' -r ./test | tap-spec",
    "start": "NODE_ENV=development babel-node src/index.js",
    "deploy": "npm run build && NODE_ENV=development serverless deploy --verbose",
    "invoke": "npm run build && NODE_ENV=development serverless invoke -f send -l"
  },
  "repository": "git@github.com:adambrgmn/sst-analytics.git",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-runtime": "^6.20.0",
    "bluebird": "^3.4.6",
    "date-fns": "^1.20.0",
    "dotenv": "^2.0.0",
    "googleapis": "^15.0.0",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.3",
    "mjml": "^3.1.1",
    "numeral": "^2.0.4",
    "pug": "^2.0.0-beta6",
    "qs": "^6.3.0",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "blue-tape": "^1.0.0",
    "cheerio": "^0.22.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
